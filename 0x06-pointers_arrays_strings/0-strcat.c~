#include <stdio.h>
#include <string.h>
#include "holberton.h"

/**
* _strcat - concatinates two strings
*
* @dest: string
* @src: string
*
* Return: dest
*/


char *_strcat(char *dest, char *src)
{
while (*dest)
{
putchar(*dest++);
}
putchar(' ');
while (*src)
{
putchar(*src++);
}
putchar('\n');
return (dest);
}
